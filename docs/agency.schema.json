{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "hospitals": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          },
          "address": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "phone",
          "address"
        ]
      }
    },
    "protocolGroups": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "keywords": {
            "type": "array",
            "items": {}
          },
          "protocols": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "standingOrders": {
                  "type": "object",
                  "properties": {
                    "basic": {
                      "type": "string"
                    },
                    "intermediate": {
                      "type": "string"
                    },
                    "paramedic": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "basic",
                    "intermediate",
                    "paramedic"
                  ]
                },
                "PEARLS": {
                  "type": "string"
                }
              },
              "required": [
                "id",
                "name",
                "standingOrders",
                "PEARLS"
              ]
            }
          }
        },
        "required": [
          "name",
          "id",
          "keywords",
          "protocols"
        ]
      }
    }
  },
  "required": [
    "hospitals",
    "protocolGroups"
  ]
}
